{
  "name": "electron-fcc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron; NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "transpile:electron": "tsc --project tsconfig.electron.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "bun run transpile:electron && bun run build && electron-builder --mac --arm64"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
